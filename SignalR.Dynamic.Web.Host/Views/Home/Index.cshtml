@model IEnumerable<SignalR.Dynamic.API.Common.Metadata>

@{
    ViewBag.Title = "Registered Topics";
}

<h2>Registered Topics</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Topic)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Topic)
        </td>
    </tr>
}
</table>
